{
  "name": "cypress-api-typescript",
  "version": "1.0.0",
  "description": "api testing using cypress + typescript",
  "main": "index.js",
  "scripts": {
    "scripts": "npx cypress run  --spec cypress/integration/spec/*.ts",
    "cypress-regression-test": "npx cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest",
    "cy-test": "npx run cypress-regression-test && npm run posttest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rindia/Cypress-api-typescript.git"
  },
  "keywords": [
    "api",
    "testing",
    "Cypress"
  ],
  "author": "Rohit saini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rindia/Cypress-api-typescript/issues"
  },
  "homepage": "https://github.com/rindia/Cypress-api-typescript#readme",
  "dependencies": {
    "typescript": "^4.6.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "devDependencies": {
    "cypress-mochawesome-reporter": "^3.0.0"
  }
}
